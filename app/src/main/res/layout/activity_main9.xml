<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dullCyan"
    tools:context=".MainActivity9">

    <TextView
        android:id="@+id/white_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/white_textview"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/announceText" />


    <TextView
    android:id="@+id/announceText"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/dullCyan"
    android:fontFamily="sans-serif"
    android:text="@string/announcement"
    android:textColor="@color/Cyan"
    android:textSize="35sp"
    android:textStyle="bold"
    android:layout_marginTop="40dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    tools:ignore="TextContrastCheck" />


    <TextView
        android:id="@+id/nav_bar_back"
        android:layout_width="0dp"
        android:layout_height="71dp"
        android:background="@drawable/nav_design"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/announce_Ibutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/round_back"
        android:importantForAccessibility="no"
        android:minWidth="48dp"
        android:padding="8dp"
        android:src="@drawable/ic_baseline_notifications_none_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/payment_Ibutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/nav_bar_back" />

    <ImageButton
        android:id="@+id/payment_Ibutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/Cyan"
        android:importantForAccessibility="no"
        android:minWidth="48dp"
        app:layout_constraintBottom_toBottomOf="@id/announce_Ibutton"
        app:layout_constraintEnd_toStartOf="@id/scan_Ibutton"
        app:layout_constraintStart_toEndOf="@id/announce_Ibutton"
        app:layout_constraintTop_toTopOf="@id/announce_Ibutton"
        app:srcCompat="@drawable/ic_payment_icon_24" />

    <ImageButton
        android:id="@+id/scan_Ibutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/Cyan"
        android:importantForAccessibility="no"
        android:minWidth="48dp"
        app:layout_constraintBottom_toBottomOf="@id/payment_Ibutton"
        app:layout_constraintEnd_toStartOf="@id/history_Ibutton"
        app:layout_constraintStart_toEndOf="@id/payment_Ibutton"
        app:layout_constraintTop_toTopOf="@id/payment_Ibutton"
        app:srcCompat="@drawable/ic_scanner_24" />

    <ImageButton
        android:id="@+id/history_Ibutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/Cyan"
        android:importantForAccessibility="no"
        android:minWidth="48dp"
        app:layout_constraintBottom_toBottomOf="@id/payment_Ibutton"
        app:layout_constraintEnd_toStartOf="@id/profile_Ibutton"
        app:layout_constraintStart_toEndOf="@id/scan_Ibutton"
        app:layout_constraintTop_toTopOf="@id/payment_Ibutton"
        app:srcCompat="@drawable/ic_history_24" />

    <ImageButton
        android:id="@+id/profile_Ibutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:background="@color/Cyan"
        android:importantForAccessibility="no"
        android:minWidth="48dp"
        android:src="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintBottom_toBottomOf="@id/history_Ibutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/history_Ibutton"
        app:layout_constraintTop_toTopOf="@id/history_Ibutton" />

    <TextView
        android:id="@+id/special_meal_back"
        android:layout_width="335dp"
        android:layout_height="118dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/announceText" />

    <ImageView
        android:id="@+id/special_menu_image"
        android:layout_width="82dp"
        android:layout_height="103dp"
        android:importantForAccessibility="no"
        app:layout_constraintStart_toStartOf="@id/special_meal_back"
        app:layout_constraintTop_toTopOf="@id/special_meal_back"
        app:layout_constraintBottom_toBottomOf="@id/special_meal_back"
        app:srcCompat="@drawable/group_47" />

    <TextView
        android:id="@+id/special_menu_text"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="@string/special_meal"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/special_meal_back"
        app:layout_constraintStart_toEndOf="@id/special_menu_image"/>

    <ImageView
        android:id="@+id/special_menu_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:importantForAccessibility="no"
        app:layout_constraintTop_toBottomOf="@id/special_meal_back"
        app:layout_constraintStart_toStartOf="@id/special_meal_back"
        app:srcCompat="@drawable/line_1"
        />

    <TextView
        android:id="@+id/desc_special_menu_announce"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:text="@string/desc_special_menu_user"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@id/special_menu_text"
        app:layout_constraintStart_toStartOf="@id/special_menu_text"/>

    <TextView
        android:id="@+id/menu_change_back"
        android:layout_width="335dp"
        android:layout_height="118dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@id/special_meal_back"
        app:layout_constraintEnd_toEndOf="@id/special_meal_back"
        app:layout_constraintTop_toBottomOf="@id/special_menu_line"/>
    <ImageView
        android:id="@+id/menu_change_image"
        android:layout_width="82dp"
        android:layout_height="103dp"
        app:srcCompat="@drawable/group_49"
        app:layout_constraintStart_toStartOf="@id/menu_change_back"
        app:layout_constraintTop_toTopOf="@id/menu_change_back"
        app:layout_constraintBottom_toBottomOf="@id/menu_change_back"
        android:importantForAccessibility="no" />

    <TextView
        android:id="@+id/menu_change_text"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="@string/menu_change"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/menu_change_back"
        app:layout_constraintStart_toEndOf="@id/menu_change_image"/>

    <TextView
        android:id="@+id/desc_menu_change_announce"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:text="@string/desc_menu_change_user"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/menu_change_text"
        app:layout_constraintTop_toBottomOf="@id/menu_change_text"/>

    <ImageView
        android:id="@+id/menu_change_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/line_1"
        android:importantForAccessibility="no"
        app:layout_constraintTop_toBottomOf="@id/menu_change_back"
        app:layout_constraintStart_toStartOf="@id/menu_change_back"/>

    <TextView
        android:id="@+id/general_mail_back"
        android:layout_width="335dp"
        android:layout_height="118dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@id/menu_change_back"
        app:layout_constraintEnd_toEndOf="@id/menu_change_back"
        app:layout_constraintTop_toBottomOf="@id/menu_change_back" />


    <ImageView
        android:id="@+id/general_mail_image"
        android:layout_width="82dp"
        android:layout_height="103dp"
        app:layout_constraintStart_toStartOf="@id/general_mail_back"
        app:layout_constraintTop_toTopOf="@id/general_mail_back"
        app:layout_constraintBottom_toBottomOf="@id/general_mail_back"
        android:importantForAccessibility="no"
        app:srcCompat="@drawable/group_50" />

    <TextView
        android:id="@+id/general_mail_text"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="@string/general_mail"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/general_mail_back"
        app:layout_constraintStart_toEndOf="@id/general_mail_image" />

    <TextView
        android:id="@+id/desc_general_mail_announce"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:text="@string/desc_general_mail_user"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/general_mail_text"
        app:layout_constraintTop_toBottomOf="@id/general_mail_text" />



</androidx.constraintlayout.widget.ConstraintLayout>